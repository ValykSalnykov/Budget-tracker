{
    "source": {
      "include": ["src", "netlify/functions"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
      "destination": "./docs/",
      "recurse": true,
      "readme": "./README.md"
    }
  }